var str = document.getElementById("hello").innerHTML;
var n = 0;

var word = "";

var check = [[2,5,6],
[1,3,5,6,7],
[2,4,6,7,8],
[3,7,8],
[1,2,5,9,10],
[1,2,3,5,7,9,10,11],
[2,3,4,6,8,10,11,12],
[3,4,7,11,12]
];

for (var i=0,i < str.length;i++) {
var c = str.charCodeAt(i);
if (c == 10) {
console.log(word);
word = "";
}
else {
word = word.concat(str.substring(i,i+1));
}
}
